<h1>New book</h1>
<%= form_with model:@book, local:true do |f| %>

  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Create Book' %>
  
  <a class="edit_275860" href="/books/275860/edit">Edit</a>
   |
  <a class="back" href="/books">Back</a>

<% end %>
